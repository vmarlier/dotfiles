<script>

(function() {
	try {
		var backToApp = document.getElementById('back_to_app');

		window.addEventListener('load', function() {
			var teamURL = getTeamURL();

			// if we managed to find a teamURL then we're inside the Slack SSB,
			// show the Back to App button
			if (backToApp && teamURL) {
				backToApp.href = teamURL;
				backToApp.style.display = 'inline';
			}
		});
	} catch(e) {
		// d'oh!
	}

	function getTeamURL(slackSSBVersion) {
		var team = null;

		try {
			team = window.desktop && window.desktop.getCurrentTeam ? window.desktop.getCurrentTeam() : null;
			if (!team) {
				
									
				var store = window.desktop && window.desktop.store ? window.desktop.store.getState() : null;
				team = store && store.teams && store.teams[window.teamId] ? store.teams[window.teamId] : null;
			}
		} catch(e) {
			team = null;
		}

		var teamURL = team && team.team_url ? team.team_url : null;

		return teamURL;
	}
}());

</script>
