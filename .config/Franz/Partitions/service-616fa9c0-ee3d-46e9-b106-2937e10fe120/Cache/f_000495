<script src="/js/jquery-2.1.4.min.js"></script>
<script>

$('[data-targeturl]').on('click', function(e) {
	var hrefLink = $(this).data('targeturl');
	var isClickTargetAChildAndALink = $(this).has(e.target).length !== 0 && e.target.nodeName.toLowerCase() === 'a';

	if (hrefLink && !isClickTargetAChildAndALink){
		window.location.href = window.location.origin + hrefLink;
	}
});

</script>

		
